{
  "name": "alfonsoar.github.io",
  "version": "0.2.0",
  "private": true,
  "homepage": "http://alfonsoaustin.com",
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "node-sass --include-path ./src --include-path   ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build"
  },
  "dependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "26.0.3",
    "@types/node": "14.0.14",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/reactstrap": "^8.4.3",
    "bootstrap": "^4.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^16.13.1",
    "reactstrap": "^8.5.1",
    "typed.js": "^2.0.11",
    "typeface-roboto": "0.0.75",
    "typeface-roboto-condensed": "0.0.75",
    "typescript": "3.9.6"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/node-sass": "^4.11.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!src/index.tsx",
      "!**/index./**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
